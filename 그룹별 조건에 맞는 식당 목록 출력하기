SELECT A.MEMBER_NAME
     , C.REVIEW_TEXT 
     , TO_CHAR(C.REVIEW_DATE, 'YYYY-MM-DD') AS REVIEW_DATE
  FROM MEMBER_PROFILE A
 INNER 
  JOIN REST_REVIEW C
    ON A.MEMBER_ID = C.MEMBER_ID
   AND EXISTS ( 
       SELECT 1
         FROM (
        SELECT MEMBER_ID 
             , COUNT(1) AS CNT 
             , DENSE_RANK() OVER(ORDER BY COUNT(1) DESC) AS RN 
          FROM REST_REVIEW  B 
         GROUP BY MEMBER_ID 
             ) B 
       WHERE B.RN = 1
         AND A.MEMBER_ID = B.MEMBER_ID
           )
ORDER BY REVIEW_DATE, REVIEW_TEXT
